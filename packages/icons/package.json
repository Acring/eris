{
  "name": "@xsky/eris-icons",
  "version": "1.26.1",
  "description": "eris icon library",
  "scripts": {
    "dev": "concurrently \"yarn dev:js:cjs\" \"yarn dev:js:esm\"",
    "figma-to-svg": "rm -rf src/svg && node ./scripts/figma-to-svg.mjs",
    "svg-to-code": "rm -rf src/icons  && node ./scripts/svg-to-code.mjs",
    "clean": "rimraf dist",
    "dev:js:cjs": "concurrently \"tsc --watch --project tsconfig.cjs.json\"  \"tsc-alias --watch -p tsconfig.cjs.json\" ",
    "dev:js:esm": "concurrently \"tsc --watch --project tsconfig.esm.json\"  \"tsc-alias --watch -p tsconfig.esm.json\" ",
    "build": "yarn clean && yarn build:js:cjs && yarn build:js:esm",
    "build:js:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:js:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "git-publish": "gitpkg publish",
    "release": "release-it --ci --no-git.requireUpstream"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@xsky/eris-eslint-config-ui": "*",
    "change-case": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "figma-js": "^1.16.0",
    "fs-extra": "^11.2.0",
    "got": "13",
    "ky": "^1.1.3",
    "p-queue": "^8.0.1",
    "react": "18",
    "react-dom": "18",
    "rimraf": "^5.0.5",
    "svgo": "^3.1.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "uppercamelcase": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint  --fix",
      "prettier --write"
    ]
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "gitpkg": {
    "registry": "git@github.xsky.com:front-end/eris.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./CHANGELOG.md": "./CHANGELOG.md",
    "./package.json": "./package.json"
  }
}
